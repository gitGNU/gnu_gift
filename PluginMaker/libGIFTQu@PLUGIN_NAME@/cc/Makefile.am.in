lib_LTLIBRARIES = libGIFTQu@PLUGIN_NAME@.la

libGIFTQu@PLUGIN_NAME@_la_LDFLAGS=@PRE_INSTALL_LIBDIRS@ -L../../libGIFTAc@PLUGIN_NAME@/cc -lMRML -lGIFTAc@PLUGIN_NAME@

